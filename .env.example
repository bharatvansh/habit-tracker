# Environment Configuration Example for Habitual App

# App Environment
NODE_ENV=development

# API Configuration
API_URL=https://api.habitual.app
API_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_DNA_SHARING=true

# Web-specific Configuration
WEB_URL=https://habitual.app
WEB_ANALYTICS_ID=UA-XXXXXXXXX-X
WEB_SENTRY_DSN=https://example@sentry.io/project

# PWA Configuration
PWA_CACHE_VERSION=v1
PWA_OFFLINE_MODE=true

# Storage Configuration
STORAGE_ENCRYPTION=false
STORAGE_PREFIX=habitual_

# External Services (Optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_PROJECT_ID=your_project_id

# Development
DEBUG=false
LOG_LEVEL=info
